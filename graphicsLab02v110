import java.awt.*;
import java.applet.*;

public class GraphicsLab03v110 extends Applet{
    public void paint(Graphics g){
        int width = 980;
        int height = 630;
        g.drawRect(10,10, width,height);
        int x1 = 990;
        int y1 = 10;
        int x2 = 10;
        int y2 = 640;
        for(int k =1; k < 100; k ++){
            g.drawLine(x1, y1, x2, y2);
            x2 += 10;
        }

    }
}
