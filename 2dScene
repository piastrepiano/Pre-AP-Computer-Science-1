	function pipe()
	{
		var myCanvas= document.getElementById("myCanvas")
		var ctx3 = myCanvas.getContext("2d");

		ctx3.fillStyle = "#567d46";
		ctx3.fillRect(200,500,100,200);
		ctx3.fill();
		
		ctx3.arc(250,500,50,0,2*Math.PI);
		ctx3.lineWidth = "10";
		ctx3.strokeStyle = "#567d46";
		ctx3.fillStyle = "green";
		ctx3.fill();
		ctx3.stroke();
	}
		
		ctx2.arc(250,250,50,8,0,175)
		ctx2.lineWidth = "10"
		ctx2.strokeStyle= "blue";
		ctx2.fillStyle = "red";
		ctx2.fill();
		ctx2.stroke();
		ctx2.fillStyle= "blue";
		ctx2.lineJoin = "round";
		ctx2.fillRect=(250,250,100,100);
</script>

<title>
2d Scene
</title>
</head>

<body onload="sky(); grass(); block1(); block2(); pipe();">
<canvas id="myCanvas" width="2000" height="1000">
</body>
</html>
